package frame;

import java.awt.BorderLayout;
import java.util.ArrayList;

import javax.swing.*;
import javax.swing.event.ListSelectionEvent;


public class FrameListeEtudiant extends JFrame {
		
	private ArrayList<String> lesvoies;
		private JList<String>listvoies;
		private JLabel lbletu;
		private String rue[];
		private JList<String>listEtudiant;
		
		public FrameListeEtudiant()
		{
			super("Liste étudiant");
						
			this.setVisible(true);
			this.pack();
			this.setVisible(true);
			this.setSize(500, 500);
			this.setLocationRelativeTo(null);
			
			this.creercomposant();			
		}
		
		public void creercomposant()
		{
		 this.lbletu=new JLabel();
		 this.lbletu.setBorder(BorderFactory.createTitledBorder( "Etudiants"));
		 this.add(this.lbletu, BorderLayout.CENTER);
		 
		 	this.listvoies=new JList<String>(this.rue);
			this.listvoies.setFixedCellHeight(20);
			this.listvoies.setFixedCellWidth(300);
			this.listvoies.setBorder(BorderFactory.createTitledBorder( "Voies"));
			this.add(this.listvoies, BorderLayout.WEST);
		}
		
		
		
		public static void main(String args[])
		{
			FrameListeEtudiant fl=new FrameListeEtudiant();
		}
}
